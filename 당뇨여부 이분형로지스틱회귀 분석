      "source": [
        "# 필요한 라이브러리 설치\n",
        "!pip install pandas statsmodels\n",
        "\n",
        "# 라이브러리 불러오기\n",
        "import pandas as pd\n",
        "import statsmodels.api as sm\n",
        "\n",
        "# CSV 파일 업로드 (Google Colab에 CSV 파일 업로드 후 경로 수정)\n",
        "from google.colab import files\n",
        "uploaded = files.upload()\n",
        "\n",
        "# 데이터 불러오기 (업로드한 파일명을 사용)\n",
        "file_path = 'Cleaned_Health_Data.csv'  # 업로드한 파일명으로 수정\n",
        "data = pd.read_csv(file_path, encoding='cp949')\n",
        "\n",
        "# 필요 없는 열 제거 (Unnamed: 0 열 제거)\n",
        "data = data.drop(columns=['Unnamed: 0'])\n",
        "\n",
        "# 독립변수와 종속변수 설정\n",
        "X = data[['허리둘레', 'BMI', '성별코드', '노인여부']]  # 독립변수\n",
        "y = data['당뇨여부']  # 종속변수 (당뇨여부)\n",
        "\n",
        "# 상수항 추가 (로지스틱 회귀분석에 필요)\n",
        "X = sm.add_constant(X)\n",
        "\n",
        "# 이분형 로지스틱 회귀 모델 적합\n",
        "logit_model = sm.Logit(y, X)\n",
        "result = logit_model.fit()\n",
        "\n",
        "# 결과 출력\n",
        "print(result.summary())\n",
        "\n"
      ]
    }
